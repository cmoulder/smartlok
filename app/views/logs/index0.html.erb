<div class="page-header">
  <h1>
    Log

    <a href="/logs/new" class="btn btn-lg btn-warning" style="float:right">Delete Log</a>

  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>

        <th>Name</th>

        <th>Access Code</th>

        <th>Time</th>

        <th>Status</th>

      </tr>

      <% @logs.reverse.each do |log| %>
      <tr>

        <td><%= log.name %></td>

        <td><%= log.accesscode %></td>

        <td><%= log.created_at %></td>

        <td><%= log.status %></td>

      </tr>
      <% end %>
    </table>
  </div>
</div>
